# Server
PORT=3000
NODE_ENV=development

# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/nodejs_tester

# Sentry (Server)
SENTRY_DSN=https://your-key@your-org.sentry.io/project-id
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=nodejs-tester@1.0.0

# Sentry (Client - Vite uses VITE_ prefix)
VITE_SENTRY_DSN=https://your-key@your-org.sentry.io/project-id
VITE_SENTRY_ENVIRONMENT=development
VITE_SENTRY_RELEASE=nodejs-tester@1.0.0

# Kubernetes context (set by Downward API in k8s)
# K8S_NODE_NAME=
# K8S_POD_NAME=
# K8S_POD_NAMESPACE=
# K8S_POD_IP=

# Seeding (optional: small, medium, large)
# SEED_SIZE=small
